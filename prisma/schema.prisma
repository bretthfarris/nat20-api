generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// ==================
// ðŸ“¦ Card Models
// ==================
model Card {
  id         Int      @id @default(autoincrement())
  game       String
  name       String
  set        String
  attributes Json
  createdAt  DateTime @default(now())
}